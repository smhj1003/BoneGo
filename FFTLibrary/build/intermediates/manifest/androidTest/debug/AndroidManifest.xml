<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.google.corp.productivity.specialprojects.android.fft.test" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="25" />

    <instrumentation
        android:name="android.test.InstrumentationTestRunner"
        android:functionalTest="false"
        android:handleProfiling="false"
        android:label="Tests for com.google.corp.productivity.specialprojects.android.fft.test"
        android:targetPackage="com.google.corp.productivity.specialprojects.android.fft.test" />
    <!-- <uses-sdk android:minSdkVersion="7" /> -->
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.BROADCAST_STICKY" />
    <uses-permission android:name="android.permission.CAPTURE_AUDIO_OUTPUT" />

    <application android:allowBackup="true" >
        <uses-library android:name="android.test.runner" />

        <activity android:name="kr.co.jjnet.angelband.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>